<?xml version="1.0" encoding="utf-8" ?>
<application
	xmlns="http://ns.adobe.com/air/application/3.0"
        minimumPatchLevel="4">
        <id>com.akangaziz.skyradar</id>
        <name>Sky Radar</name>
        <versionNumber>0.1</versionNumber>
        <filename>Sky Radar</filename>
	<description>SkyRadar Apps for flight and plane monitoring and tracking .</description>
	<copyright>Copyright 2013. akangaziz Incorporated. All rights reserved.</copyright>
	<initialWindow>
		<content>index.html</content>
		<title>OK</title>
		<systemChrome>none</systemChrome>
		<transparent>false</transparent>
		<visible>true</visible>
		<minimizable>true</minimizable>
		<maximizable>true</maximizable>
		<resizable>true</resizable>
		<fullScreen>true</fullScreen>
		<!--<x>300</x>
		<y>250</y>
		<minSize>500 500</minSize>-->
	</initialWindow>
    <installFolder>Sky Radar</installFolder>
    <programMenuFolder>Sky Radar</programMenuFolder>
    <icon>
		<image16x16>js/AIRsrc/icons/16.png</image16x16>
		<image32x32>js/AIRsrc/icons/32.png</image32x32>
		<image48x48>js/AIRsrc/icons/48.png</image48x48>
		<image128x128>js/AIRsrc/icons/128.png</image128x128>
	</icon>
	<customUpdateUI>false</customUpdateUI>
	<allowBrowserInvocation>false</allowBrowserInvocation>
</application>